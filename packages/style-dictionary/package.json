{
  "name": "@faciocode/style-dictionary",
  "displayName": "FacioStyleDictionary",
  "private": false,
  "version": "21.0.1",
  "description": "Styles and assets for Facio Design System",
  "keywords": [
    "styles",
    "facio",
    "design",
    "ds"
  ],
  "author": {
    "name": "Facio",
    "url": "https://github.com/FacioCode"
  },
  "homepage": "https://github.com/FacioCode/design/tree/main/packages/styles#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "directories": {
    "lib": "dist"
  },
  "main": "build/index.js",
  "source": "src/index.js",
  "repository": {
    "directory": "packages/style-dictionary",
    "type": "git",
    "url": "https://github.com/FacioCode/design"
  },
  "bugs": {
    "url": "https://github.com/FacioCode/design/issues"
  },
  "devDependencies": {
    "style-dictionary": "3.0.1"
  },
  "scripts": {
    "build": "npm run compile && npm run style-dictionary",
    "clean": "rm -rf ./build && rm -rf ./dist && rm -rf ../flutter_ds/lib/src/generated",
    "compile": "tsc",
    "prebuild": "npm run svgo",
    "style-dictionary": "node ./build/index.js",
    "svgo": "svgo -f assets/icons --config=./src/config/svgo.json --quiet",
    "test": "jest",
    "tsc": "tsc",
    "webfont": "webfont"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.17.3 || >=16.4.0",
    "npm": ">=7.19.0"
  }
}
