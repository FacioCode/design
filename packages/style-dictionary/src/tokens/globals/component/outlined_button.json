{
  "component": {
    "outlinedButton": {
      "root": {
        "comment": "Styles applied to the root element if variant is outlined",
        "borderWidth": { "value": "{size.border.width.single.value}" }
      },
      "default": {
        "borderColor": { "value": "{color.border.button.outlined.value}" },
        "color": { "value": "{color.font.primary.value}" }
      },
      "brand": {
        "borderColor": { "value": "{component.outlinedButton.default.borderColor.value}" },
        "color": { "value": "{component.outlinedButton.default.color.value}" }
      },
      "brandHover": {
        "borderColor": { "value": "{color.base.primary.main.value}" }
      },
      "sizeSmall": {
        "comment": "Styles applied to the root element if size is small and variant is outlined.",
        "borderRadius": { "value": "{size.border.radius.double.value}" },
        "paddingBottom": { "value": "{size.base.quarter.value}" },
        "paddingTop": { "value": "{size.base.quarter.value}" }
      }
    }
  }
}
