{
  "component": {
    "outlinedButton": {
      "comment": "There are no color variations for outlined button.",
      "root": {
        "comment": "Styles applied to the root element if variant is outlined",
        "borderWidth": { "value": "{size.border.width.single.value}" }
      },
      "default": {
        "backgroundColor": { "value": "{color.base.transparent.00.value}" },
        "borderColor": { "value": "{color.border.outlinedButton.value}" },
        "color": { "value": "{color.font.primaryHighContrast.value}" }
      },
      "disabled": {
        "backgroundColor": { "value": "{component.outlinedButton.default.backgroundColor.value}" },
        "borderColor": { "value": "{component.outlinedButton.default.borderColor.value}" },
        "color": {
          "comment": "Font color for outlined disabled buttons follows theme disabled font color",
          "value": "{color.font.disabled.value}"
        }
      },
      "focusVisible": {
        "backgroundColor": { "value": "{color.base.transparent.00.value}" },
        "borderColor": { "value": "{component.outlinedButton.default.borderColor.value}" },
        "outlineWidth": { "value": "{size.base.none.value}" }
      },
      "pressed": {
        "backgroundColor": { "value": "{color.base.transparent.05.value}" },
        "borderColor": { "value": "{color.border.outlinedButtonPressed.value}" },
        "color": { "value": "{component.outlinedButton.default.color.value}" }
      },
      "hover": {
        "backgroundColor": { "value": "{color.base.transparent.00.value}" },
        "borderColor": { "value": "{color.border.outlinedButtonPressed.value}" }
      }
    }
  }
}
