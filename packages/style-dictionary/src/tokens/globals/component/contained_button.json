{
  "component": {
    "containedButton": {
      "root": {
        "comment": "Styles applied to the root element if component is a contained button.",
        "borderWidth": { "value": "{size.border.width.none.value}" }
      },
      "brand": {
        "backgroundColor": { "value": "{color.base.primary.main.value}" },
        "color": { "value": "{color.font.primary.value}" }
      },
      "brandDisabled": {
        "backgroundColor": { "value": "{color.base.primary.lighter.value}" },
        "color": {
          "comment": "Font color for disabled contained buttons uses secondary instead of disabled",
          "value": "{color.font.secondary.value}"
        }
      },
      "brandHover": {
        "backgroundColor": { "value": "{color.base.primary.main.value}" },
        "color": { "value": "{color.font.primary.value}" }
      },
      "brandPressed": {
        "backgroundColor": { "value": "{color.base.primary.dark.value}" },
        "color": { "value": "{color.font.primary.value}" }
      },
      "danger": {
        "backgroundColor": { "value": "{color.base.error.main.value}" },
        "color": { "value": "{color.font.inverse.value}" }
      },
      "dangerHover": {
        "backgroundColor": { "value": "{component.containedButton.danger.backgroundColor.value}" },
        "color": { "value": "{color.font.inverse.value}" }
      },
      "dangerPressed": {
        "backgroundColor": { "value": "{color.base.error.dark.value}" },
        "color": { "value": "{color.font.inverse.value}" }
      },
      "default": {
        "backgroundColor": { "value": "{color.base.secondary.main.value}" },
        "color": { "value": "{color.font.inverse.value}" }
      },
      "defaultHover": {
        "backgroundColor": { "value": "{component.containedButton.default.backgroundColor.value}" },
        "color": { "value": "{component.containedButton.default.color.value}" }
      },
      "defaultPressed": {
        "backgroundColor": { "value": "{color.base.secondary.dark.value}" },
        "color": { "value": "{color.font.inverse.value}" }
      },
      "focusVisible": {
        "outlineWidth": { "value": "{size.base.none.value}" }
      },
      "warning": {
        "backgroundColor": { "value": "{color.base.warning.main.value}" },
        "color": { "value": "{color.font.primaryHighContrast.value}" }
      },
      "warningHover": {
        "backgroundColor": { "value": "{color.base.warning.main.value}" },
        "color": { "value": "{color.font.primaryHighContrast.value}" }
      },
      "warningPressed": {
        "backgroundColor": { "value": "{color.base.warning.dark.value}" },
        "color": { "value": "{color.font.primaryHighContrast.value}" }
      }
    }
  }
}
